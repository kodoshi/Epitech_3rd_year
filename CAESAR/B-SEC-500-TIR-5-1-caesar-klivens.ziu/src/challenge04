def throw(msg, name=sys.argv[0]):
    print('%s:' % name, msg, file=sys.stderr)
    sys.exit(84)


def main(args):
    if len(args) != 2:
        throw('Invalid arguments')
    try:
        with open(args[1], 'r') as f:
            data = [bytes.fromhex(re.sub(r'\s', '', line)) for line in f]
            if len(data) == 0 or any(len(d) == 0 for d in data):
                raise ValueError()
    except IOError as e:
        throw('Cannot read file: %s' % e.strerror, name=args[1])
    except ValueError:
        throw('Invalid file context', name=args[1])

    line, key = max(((line, key) for line in range(len(data)) for key in range(256)), key=lambda args: sum(
        ETAOIN[byte] for byte in map(lambda byte: chr(byte ^ args[1]), data[args[0]]) if byte in ETAOIN))
    print(line + 1, bytes([key]).hex().upper())


if __name__ == '__main__':
    main(sys.argv)
